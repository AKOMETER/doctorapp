{
  "expo": {
    "name": "parcel-tracking-rn",
    "slug": "parcel-tracking-rn",
    "entryPoint": "./app/index.tsx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.striker1101.parceltrackingrn"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_BACKEND_URL": "https://doctorwelfare.erose.name.ng",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f4e6a937-251d-4f61-90f2-2f314dfa4cc4"
      }
    }
  }
}
